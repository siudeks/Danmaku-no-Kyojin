<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml type="vertical">
  <action label="XWay">
    <actionRef label="XWayFan">
      <param>$1</param>
      <param>$2</param>
      <param>0</param>
    </actionRef>
  </action>
  <action label="XWayFan">
    <repeat>
      <times>1</times>
      <action>
        <fire>
          <direction type="sequence">$2</direction>
          <speed type="sequence">$3</speed>
          <bullet/>
        </fire>
      </action>
    </repeat>
  </action>
  <action label="fan4">
    <fire>
      <direction type="absolute">-$1</direction>
	  <bullet />
      <vanish/>
    </fire>
    <repeat>
      <times>30+$rank*45</times>
      <action>
        <fire>
          <direction type="sequence">$1</direction>
          <speed>1+$rank*$rank*2</speed>
          <bullet/>
        </fire>
        <actionRef label="XWay">
          <param>0</param>
          <param>90</param>
        </actionRef>
        <wait>6-$rank*3</wait>
      </action>
    </repeat>
  </action>
  <action label="top2">
    <actionRef label="fan4">
      <param>4</param>
    </actionRef>
  </action>
  <action label="top3">
    <actionRef label="fan4">
      <param>-4</param>
    </actionRef>
  </action>
</bulletml>